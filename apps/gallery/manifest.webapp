{
  "fullscreen": true, 
  "name": "Gallery", 
  "default_locale": "en-US", 
  "activities": {
    "browse": {
      "disposition": "window", 
      "filters": {
        "type": "photos"
      }
    }, 
    "open": {
      "href": "/open.html", 
      "returnValue": true, 
      "disposition": "inline", 
      "filters": {
        "type": [
          "image/jpeg", 
          "image/png", 
          "image/gif", 
          "image/bmp"
        ]
      }
    }, 
    "pick": {
      "href": "/index.html#pick", 
      "returnValue": true, 
      "disposition": "inline", 
      "filters": {
        "type": [
          "image/*", 
          "image/jpeg", 
          "image/png"
        ]
      }
    }
  }, 
  "description": "Gaia Gallery", 
  "icons": {
    "60": "/style/icons/Gallery_60.png", 
    "90": "/style/icons/Gallery_90.png", 
    "135": "/style/icons/Gallery_135.png", 
    "120": "/style/icons/Gallery_120.png"
  }, 
  "launch_path": "/index.html", 
  "permissions": {
    "audio-channel-content": {}, 
    "device-storage:pictures": {
      "access": "readwrite"
    }, 
    "nfc": {
      "access": "readwrite"
    }, 
    "settings": {
      "access": "readonly"
    }, 
    "device-storage:videos": {
      "access": "readwrite"
    }, 
    "storage": {}, 
    "deprecated-hwvideo": {}
  }, 
  "type": "certified", 
  "locales": {
    "zh-TW": {
      "name": "\u5716\u7247\u5eab", 
      "description": "Gaia \u5716\u7247\u5eab"
    }, 
    "en-US": {
      "name": "Gallery", 
      "description": "Gaia Gallery"
    }, 
    "ar": {
      "name": "\u0627\u0644\u0645\u0639\u0631\u0636", 
      "description": "\u0627\u0644\u0645\u0639\u0631\u0636"
    }, 
    "fr": {
      "name": "Galerie", 
      "description": "Galerie de Gaia"
    }
  }, 
  "developer": {
    "url": "https://github.com/mozilla-b2g/gaia", 
    "name": "The Gaia Team"
  }
}