{
  "activities": {
    "dial": {
      "href": "/dialer/index.html#keyboard-view", 
      "disposition": "window", 
      "filters": {
        "type": "webtelephony/number", 
        "number": {
          "pattern": "[\\d\\s+#*().-]{0,50}"
        }
      }
    }, 
    "pick": {
      "returnValue": true, 
      "href": "/contacts/index.html?pick", 
      "disposition": "inline", 
      "filters": {
        "type": {
          "required": true, 
          "value": [
            "webcontacts/contact", 
            "webcontacts/email", 
            "webcontacts/tel"
          ]
        }
      }
    }, 
    "update": {
      "returnValue": true, 
      "href": "/contacts/index.html?update", 
      "disposition": "inline", 
      "filters": {
        "type": "webcontacts/contact"
      }
    }, 
    "new": {
      "returnValue": true, 
      "href": "/contacts/index.html?new", 
      "disposition": "inline", 
      "filters": {
        "type": "webcontacts/contact"
      }
    }, 
    "import": {
      "returnValue": true, 
      "href": "/contacts/index.html?open", 
      "disposition": "inline", 
      "filters": {
        "type": "text/vcard"
      }
    }, 
    "open": {
      "returnValue": true, 
      "href": "/contacts/index.html?open", 
      "disposition": "inline", 
      "filters": {
        "type": "webcontacts/contact"
      }
    }
  }, 
  "entry_points": {
    "dialer": {
      "launch_path": "/dialer/index.html#keyboard-view", 
      "name": "Phone", 
      "locales": {
        "fr": {
          "name": "T\u00e9l\u00e9phone", 
          "description": "T\u00e9l\u00e9phone de Gaia"
        }, 
        "zh-TW": {
          "name": "\u96fb\u8a71", 
          "description": "Gaia \u96fb\u8a71"
        }, 
        "en-US": {
          "name": "Phone", 
          "description": "Gaia Phone"
        }, 
        "it": {
          "name": "Telefono", 
          "description": "Telefono Gaia"
        }, 
        "ar": {
          "name": "\u0647\u0627\u062a\u0641", 
          "description": "Gaia \u0647\u0627\u062a\u0641"
        }, 
        "ro": {
          "name": "Telefon", 
          "description": "Telefon Gaia"
        }
      }, 
      "icons": {
        "60": "/dialer/style/icons/Dialer_60.png", 
        "90": "/dialer/style/icons/Dialer_90.png", 
        "120": "/dialer/style/icons/Dialer_120.png"
      }
    }, 
    "ftu": {
      "fullscreen": "true", 
      "launch_path": "/ftu/index.html", 
      "name": "FTU"
    }, 
    "contacts": {
      "launch_path": "/contacts/index.html", 
      "name": "Contacts", 
      "locales": {
        "fr": {
          "name": "Contacts", 
          "description": "Contacts de Gaia"
        }, 
        "zh-TW": {
          "name": "\u806f\u7d61\u4eba", 
          "description": "Gaia \u806f\u7d61\u4eba"
        }, 
        "en-US": {
          "name": "Contacts", 
          "description": "Gaia Contacts"
        }, 
        "it": {
          "name": "Contatti", 
          "description": "Contatti Gaia"
        }, 
        "ar": {
          "name": "\u0627\u062a\u0635\u0627\u0644\u0627\u062a", 
          "description": "Gaia \u0627\u062a\u0635\u0627\u0644\u0627\u062a"
        }, 
        "ro": {
          "name": "Contacte", 
          "description": "Contacte Gaia"
        }
      }, 
      "icons": {
        "60": "/contacts/style/icons/Contacts_60.png", 
        "90": "/contacts/style/icons/Contacts_90.png", 
        "120": "/contacts/style/icons/Contacts_120.png"
      }
    }
  }, 
  "name": "Communications", 
  "orientation": "default", 
  "description": "Gaia Communications", 
  "messages": [
    {
      "alarm": "/facebook/fb_sync.html"
    }, 
    {
      "bluetooth-dialer-command": "/dialer/index.html#keyboard-view"
    }, 
    {
      "headset-button": "/dialer/index.html#keyboard-view"
    }, 
    {
      "notification": "/dialer/index.html#keyboard-view"
    }, 
    {
      "telephony-new-call": "/dialer/index.html#keyboard-view"
    }, 
    {
      "ussd-received": "/dialer/index.html#keyboard-view"
    }
  ], 
  "redirects": [
    {
      "to": "/redirects/redirect.html", 
      "from": "facebook"
    }, 
    {
      "to": "/redirects/dialogs_end.html", 
      "from": "facebook_dialogs"
    }, 
    {
      "to": "/redirects/logout.json", 
      "from": "facebook_logout"
    }, 
    {
      "to": "/redirects/redirect.html", 
      "from": "live"
    }, 
    {
      "to": "/redirects/redirect.html", 
      "from": "gmail"
    }
  ], 
  "datastores-owned": {
    "Gaia_Facebook_Friends": {
      "description": "Imported Facebook Friends"
    }
  }, 
  "launch_path": "/", 
  "permissions": {
    "device-storage:sdcard": {
      "access": "readcreate"
    }, 
    "desktop-notification": {}, 
    "wifi-manage": {}, 
    "contacts": {
      "access": "readwrite"
    }, 
    "voicemail": {}, 
    "attention": {}, 
    "storage": {}, 
    "idle": {}, 
    "audio-channel-telephony": {}, 
    "bluetooth": {}, 
    "telephony": {}, 
    "systemXHR": {}, 
    "audio-channel-ringer": {}, 
    "time": {}, 
    "alarms": {}, 
    "mobileconnection": {}, 
    "settings": {
      "access": "readwrite"
    }
  }, 
  "type": "certified", 
  "developer": {
    "url": "https://github.com/mozilla-b2g/gaia", 
    "name": "The Gaia Team"
  }
